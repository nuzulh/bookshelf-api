{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "indent": ["error", 4],
        "no-else-return": "off",
        "max-len": "off",
        "arrow-body-style": "off"
    }
}
